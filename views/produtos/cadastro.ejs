<script src="/js/produtos/cadastrar.js"></script>

<div class="container">
    <h1>Cadastro de Produtos</h1>
    <div>
        <% if (typeof msg !="undefined" ) { %>
            <span>
                <%= msg %>
            </span>
            <% } %>
    </div>
    <div>
        <div>
            <div class="form-group">
                <label for="produtoNome" class="form-label">Nome</label>
                <input type="text" class="form-control" id="produtoNome" name="produtoNome">
            </div>
        </div>
        <div>
            <div class="form-group">
                <label for="produtoDesc" class="form-label">Descrição</label>
                <input type="text" class="form-control" id="produtoDesc" name="produtoDesc">
            </div>
        </div>
        <div>
            <div class="form-group">
                <label for="produtoPreco" class="form-label">Preço</label>
                <input type="text" class="form-control" id="produtoPreco" name="produtoPreco">
            </div>
        </div>
        <div>
            <div class="form-group">
                <label for="produtoQuant" class="form-label">Quantidade</label>
                <input type="text" class="form-control" id="produtoQuant" name="produtoQuant">
            </div>
        </div>
        <div>
            <button class="btn btn-primary" id="btnCadastrar" type="button">Cadastrar</button>
            <a class="btn btn-secondary" href="/produtos">Voltar</a>
        </div>
    </div>
</div>
</div>